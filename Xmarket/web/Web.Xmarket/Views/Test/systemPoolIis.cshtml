@model Web.Xmarket.Models.Test.IISInfoViewModel

@{

    Layout = null;
    ViewBag.Title = "Información de IIS";
}


<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> @ViewBag.Title </title>
    <meta name="robots" content="noindex, nofollow">

    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 20px;
            padding: 20px;
            background-color: #f4f4f4;
        }

        .container {
            max-width: 600px;
            background: white;
            padding: 20px;
            border-radius: 10px;
            box-shadow: 0px 0px 10px rgba(0,0,0,0.1);
        }

        h2 {
            color: #333;
        }

        .info {
            font-size: 18px;
            margin: 10px 0;
        }

        .highlight {
            font-weight: bold;
            color: #007bff;
        }
    </style>
</head>
<body>

    <h2>Información de IIS</h2>

    <h3>Application Pools</h3>
    <ul>
        @foreach (var pool in Model.ApplicationPools)
        {
            <li>@pool</li>
        }
    </ul>

    <h3>Sitios Web</h3>
    <ul>
        @foreach (var sitio in Model.Websites)
        {
            <li>@sitio</li>
        }
    </ul>

</body>
</html>
